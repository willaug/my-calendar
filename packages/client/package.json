{
  "name": "my-calendar-client",
  "description": "client of my-calendar project",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "build": "ng build",
    "commit": "cd .. && npm run commit",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --no-watch --browsers=ChromeHeadless",
    "test:api": "cd ../server && npm t",
    "start-api-dev": "cd ../server && npm run dev",
    "test-api-and-start-app": "npm run test:api && npm run start-api-and-app",
    "e2e:watch": "npm run start-api-and-app \"npx cypress open\"",
    "start-api-and-app": "concurrently --r --kill-others \"npm run start-api-dev\" \"npm start\""
  },
  "keywords": [
    "client",
    "angular",
    "angular13",
    "my-calendar",
    "MyCalendar",
    "MY-CALENDAR"
  ],
  "author": "William Augusto <william.santos315@outlook.com>",
  "license": "ISC",
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/cdk": "~13.1.1",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/flex-layout": "^13.0.0-beta.37",
    "@angular/forms": "~13.1.0",
    "@angular/material": "~13.1.1",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/router": "~13.1.0",
    "@apollo/client": "^3.0.0",
    "apollo-angular": "^3.0.0-alpha.2",
    "change-case": "^4.1.2",
    "graphql": "^15.8.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.2",
    "@angular-eslint/builder": "13.0.1",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/schematics": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/cli": "~13.1.2",
    "@angular/compiler-cli": "~13.1.0",
    "@cypress/schematic": "^1.6.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "concurrently": "^7.0.0",
    "cypress": "^9.2.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.5.2"
  }
}
