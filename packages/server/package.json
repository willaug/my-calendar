{
  "name": "my-calendar-server",
  "version": "1.0.0",
  "description": "server do projeto my-calendar",
  "main": "server.js",
  "scripts": {
    "start-prod": "npm run build && node dist/server",
    "start-dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/server.ts",
    "commit": "cd .. && npm run commit",
    "test": "npm run create-db && jest --detectOpenHandles --passWithNoTests",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "create-db": "ts-node-dev -r tsconfig-paths/register --transpile-only --ignore-watch node_modules --no-notify src/core/database/scripts/create-db.ts",
    "migrate-make": "npx knex --knexfile src/core/database/config.ts migrate:make -x ts"
  },
  "keywords": [
    "server",
    "api",
    "graphql",
    "my-calendar",
    "MyCalendar",
    "MY-CALENDAR"
  ],
  "author": "William Augusto <william.santos315@outlook.com>",
  "license": "ISC",
  "dependencies": {
    "chalk": "^4.1.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "knex": "^0.95.11",
    "pg": "^8.7.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/node": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/pg": "^8.6.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "babel-jest": "^27.2.5",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.1",
    "jest": "^27.2.5",
    "ts-node": "^10.3.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.3"
  }
}
